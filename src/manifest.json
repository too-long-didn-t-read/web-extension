{
  "name": "Chrome Extension Webpack",
  "options_ui": {
    "page": "options.html"
  },
  "background": {
    "scripts": [
      "background.bundle.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "version": "1.0",
  "permissions": [
    "activeTab",
    "tabs",
    "https://tldr-sails.herokuapp.com/",
    "https://stackpath.bootstrapcdn.com/",
    "https://fonts.googleapis.com/",
    "https://kit.fontawesome.com/"
  ]
}
